{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hocon",
      "camelcase": "Hocon",
      "title": "Hocon",
      "scope": "source.hocon",
      "file-types": [
        "hocon"
      ],
      "injection-regex": "^hocon$",
      "class-name": "TreeSitterHocon"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Hocon grammar for tree-sitter",
    "authors": [
      {
        "name": "Zed"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-hocon"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}